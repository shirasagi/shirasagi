<http://imi.ipa.go.jp/ns/core/rdf> a owl:Ontology ;
	rdfs:label "共通語彙基盤コア語彙"@ja, "IMI Core Vocabulary"@en ;
	dct:creator [
		foaf:homepage <http://www.ipa.go.jp/> ;
		foaf:name "IPA 独立行政法人情報処理推進機構"@ja, "Information-technology Promotion Agency, Japan."@en
	], [
		foaf:homepage <http://www.meti.go.jp/> ;
		foaf:name "経済産業省"@ja, "Ministry of Economy, Trade and Industry"@en
	] ;
	dct:issued "2015-02-03" ;
	cc:license <http://creativecommons.org/publicdomain/zero/1.0/> ;
	rdfs:comment "コア語彙は、共通語彙基盤の基礎をなすもので、氏名、住所、組織等、あらゆる社会活動で使用される中核的な用語の集合です。多くのシステム間で情報交換のための基礎となる語彙で、データ交換、オープンデータの二次利用等の効率化に役立つものです。"@ja ;
	owl:versionInfo "2.2" .


## プロパティ定義 ################
ic:ID a owl:ObjectProperty ;
	rdfs:label "ID"@ja ;
	rdfs:comment "事物を識別するためのID型の識別子"@ja ;
	schema:domainIncludes ic:人型, ic:住所型, ic:連絡先型, ic:組織型, ic:製品型, ic:地物型, ic:設備型, ic:イベント型, ic:アクセス型 ;
	rdfs:range ic:ID型 .

ic:氏名 a owl:ObjectProperty ;
	rdfs:label "氏名"@ja ;
	rdfs:comment "人の氏名"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:氏名型 .

ic:性別 a owl:DatatypeProperty ;
	rdfs:label "性別"@ja ;
	rdfs:comment "性別の表記"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range xsd:string .

ic:性別コード a owl:ObjectProperty ;
	rdfs:label "性別コード"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:コード型 .

ic:生年月日 a owl:ObjectProperty ;
	rdfs:label "生年月日"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:日付型 .

ic:死亡年月日 a owl:ObjectProperty ;
	rdfs:label "死亡年月日"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:日付型 .

ic:住所 a owl:ObjectProperty ;
	rdfs:label "住所"@ja ;
	rdfs:comment "現住所など"@ja ;
	schema:domainIncludes ic:人型, ic:連絡先型, ic:組織型, ic:場所型 ;
	rdfs:range ic:住所型 .

ic:本籍 a owl:ObjectProperty ;
	rdfs:label "本籍"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:住所型 .

ic:連絡先 a owl:ObjectProperty ;
	rdfs:label "連絡先"@ja ;
	rdfs:comment "組織の連絡先（本社、コールセンター等）など"@ja ;
	schema:domainIncludes ic:人型, ic:組織型, ic:地物型, ic:設備型, ic:イベント型 ;
	rdfs:range ic:連絡先型 .

ic:国籍 a owl:DatatypeProperty ;
	rdfs:label "国籍"@ja ;
	rdfs:comment "国籍の表記"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range xsd:string .

ic:国籍コード a owl:ObjectProperty ;
	rdfs:label "国籍コード"@ja ;
	rdfs:comment "住民基本台帳で利用されている国籍コード"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:コード型 .

ic:出生国 a owl:DatatypeProperty ;
	rdfs:label "出生国"@ja ;
	rdfs:comment "生まれた国名"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range xsd:string .

ic:出生国コード a owl:ObjectProperty ;
	rdfs:label "出生国コード"@ja ;
	rdfs:comment "生まれた国のコード"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:コード型 .

ic:出生地 a owl:ObjectProperty ;
	rdfs:label "出生地"@ja ;
	rdfs:comment "生まれた場所"@ja ;
	schema:domainIncludes ic:人型 ;
	rdfs:range ic:住所型 .

ic:種別 a owl:DatatypeProperty ;
	rdfs:label "種別"@ja ;
	rdfs:comment "様々な事物の種類の表記"@ja ;
	schema:domainIncludes ic:名称型, ic:氏名型, ic:住所型, ic:連絡先型, ic:人数型, ic:製品型, ic:施設型, ic:イベント型, ic:価格型, ic:定期スケジュール型, ic:アクセス型, ic:アクセス区間型 ;
	rdfs:range xsd:string .

ic:表記 a owl:DatatypeProperty ;
	rdfs:label "表記"@ja ;
	rdfs:comment "日本語による表記"@ja ;
	schema:domainIncludes ic:名称型, ic:住所型 ;
	rdfs:range xsd:string .

ic:カナ表記 a owl:DatatypeProperty ;
	rdfs:label "カナ表記"@ja ;
	rdfs:comment "カタカナ表記"@ja ;
	schema:domainIncludes ic:名称型 ;
	rdfs:range xsd:string .

ic:ローマ字表記 a owl:DatatypeProperty ;
	rdfs:label "ローマ字表記"@ja ;
	schema:domainIncludes ic:名称型 ;
	rdfs:range xsd:string .

ic:姓名 a owl:DatatypeProperty ;
	rdfs:label "姓名"@ja ;
	rdfs:comment "氏名（姓、名）の表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:姓名カナ表記 a owl:DatatypeProperty ;
	rdfs:label "姓名カナ表記"@ja ;
	rdfs:comment "氏名（姓、名）のカナ表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:姓名ローマ字表記 a owl:DatatypeProperty ;
	rdfs:label "姓名ローマ字表記"@ja ;
	rdfs:comment "氏名（姓、名）のアルファベット表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:姓 a owl:DatatypeProperty ;
	rdfs:label "姓"@ja ;
	rdfs:comment "姓の表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:姓カナ表記 a owl:DatatypeProperty ;
	rdfs:label "姓カナ表記"@ja ;
	rdfs:comment "姓のカナ表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:姓ローマ字表記 a owl:DatatypeProperty ;
	rdfs:label "姓ローマ字表記"@ja ;
	rdfs:comment "姓のローマ表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:名 a owl:DatatypeProperty ;
	rdfs:label "名"@ja ;
	rdfs:comment "名の表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:名カナ表記 a owl:DatatypeProperty ;
	rdfs:label "名カナ表記"@ja ;
	rdfs:comment "名のカナ表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:名ローマ字表記 a owl:DatatypeProperty ;
	rdfs:label "名ローマ字表記"@ja ;
	rdfs:comment "名のローマ字表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:ミドルネーム a owl:DatatypeProperty ;
	rdfs:label "ミドルネーム"@ja ;
	rdfs:comment "ミドルネームの表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:ミドルネームカナ表記 a owl:DatatypeProperty ;
	rdfs:label "ミドルネームカナ表記"@ja ;
	rdfs:comment "ミドルネームのカナ表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:ミドルネームローマ字表記 a owl:DatatypeProperty ;
	rdfs:label "ミドルネームローマ字表記"@ja ;
	rdfs:comment "ミドルネームのローマ字表記"@ja ;
	schema:domainIncludes ic:氏名型 ;
	rdfs:range xsd:string .

ic:郵便番号 a owl:DatatypeProperty ;
	rdfs:label "郵便番号"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

ic:住所コード a owl:ObjectProperty ;
	rdfs:label "住所コード"@ja ;
	rdfs:comment "地方公共団体情報システム機構地方公共団体コード住所等の利用を想定。住所のコード化が必要な範囲に応じて自治体で個別に設定する"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range ic:コード型 .

ic:国 a owl:DatatypeProperty ;
	rdfs:label "国"@ja ;
	rdfs:comment "国名"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

