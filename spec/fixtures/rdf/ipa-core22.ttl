## クラスのプロパティ制約定義 ################
ic:人型 a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty ic:ID ;
		owl:allValuesFrom ic:ID型
	], [
		a owl:Restriction ;
		owl:onProperty ic:氏名 ;
		owl:allValuesFrom ic:氏名型
	], [
		a owl:Restriction ;
		owl:onProperty ic:性別 ;
		owl:onDataRange xsd:string ;
		owl:maxQualifiedCardinality 1 ;
		rdfs:comment "性別の表記"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:性別コード ;
		owl:onClass ic:コード型 ;
		owl:maxQualifiedCardinality 1
	], [
		a owl:Restriction ;
		owl:onProperty ic:生年月日 ;
		owl:onClass ic:日付型 ;
		owl:maxQualifiedCardinality 1
	], [
		a owl:Restriction ;
		owl:onProperty ic:死亡年月日 ;
		owl:onClass ic:日付型 ;
		owl:maxQualifiedCardinality 1
	], [
		a owl:Restriction ;
		owl:onProperty ic:住所 ;
		owl:allValuesFrom ic:住所型 ;
		rdfs:comment "現住所"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:本籍 ;
		owl:onClass ic:住所型 ;
		owl:maxQualifiedCardinality 1
	], [
		a owl:Restriction ;
		owl:onProperty ic:連絡先 ;
		owl:allValuesFrom ic:連絡先型
	], [
		a owl:Restriction ;
		owl:onProperty ic:国籍 ;
		owl:allValuesFrom xsd:string ;
		rdfs:comment "国籍の表記"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:国籍コード ;
		owl:allValuesFrom ic:コード型 ;
		rdfs:comment "住民基本台帳で利用されている国籍コード"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:出生国 ;
		owl:onDataRange xsd:string ;
		owl:maxQualifiedCardinality 1 ;
		rdfs:comment "生まれた国名"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:出生国コード ;
		owl:onClass ic:コード型 ;
		owl:maxQualifiedCardinality 1 ;
		rdfs:comment "生まれた国のコード"@ja
	], [
		a owl:Restriction ;
		owl:onProperty ic:出生地 ;
		owl:onClass ic:住所型 ;
		owl:maxQualifiedCardinality 1 ;
		rdfs:comment "生まれた場所"@ja
	] .

ic:名称型 a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty ic:種別 ;
		owl:onDataRange xsd:string ;
