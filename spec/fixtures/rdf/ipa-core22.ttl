	rdfs:range xsd:string .

ic:方書 a owl:DatatypeProperty ;
	rdfs:label "方書"@ja ;
	rdfs:comment "方書 (私書箱も含む)"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

ic:名称 a owl:ObjectProperty ;
	rdfs:label "名称"@ja ;
	rdfs:comment "事物の名称"@ja ;
	schema:domainIncludes ic:連絡先型, ic:組織型, ic:場所型, ic:ID体系型, ic:製品型, ic:設備型, ic:イベント型, ic:状況型, ic:コードリスト型 ;
	rdfs:range ic:名称型 .

ic:組織 a owl:ObjectProperty ;
	rdfs:label "組織"@ja ;
	rdfs:comment "事物に関連する組織"@ja ;
	schema:domainIncludes ic:連絡先型, ic:組織関連型 ;
