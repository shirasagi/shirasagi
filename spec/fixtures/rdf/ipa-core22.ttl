	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

ic:都道府県コード a owl:ObjectProperty ;
	rdfs:label "都道府県コード"@ja ;
	rdfs:comment "都道府県のコード"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range ic:コード型 .

ic:市区町村 a owl:DatatypeProperty ;
	rdfs:label "市区町村"@ja ;
	rdfs:comment "市区町村名"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

ic:区 a owl:DatatypeProperty ;
	rdfs:label "区"@ja ;
