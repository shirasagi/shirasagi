ic:対応言語 a owl:DatatypeProperty ;
	rdfs:label "対応言語"@ja ;
	rdfs:comment "連絡先との連絡の際に利用可能な言語名"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range xsd:string .

ic:略称アルファベット a owl:DatatypeProperty ;
