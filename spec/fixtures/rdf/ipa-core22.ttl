ic:ビル名 a owl:DatatypeProperty ;
	rdfs:label "ビル名"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range xsd:string .

ic:ビル番号 a owl:DatatypeProperty ;
