	rdfs:range xsd:string .

ic:市区町村コード a owl:ObjectProperty ;
	rdfs:label "市区町村コード"@ja ;
	rdfs:comment "市区町村のコード"@ja ;
	schema:domainIncludes ic:住所型 ;
	rdfs:range ic:コード型 .

ic:町名 a owl:DatatypeProperty ;
	rdfs:label "町名"@ja ;
