ic:担当者役職 a owl:DatatypeProperty ;
	rdfs:label "担当者役職"@ja ;
	rdfs:comment "連絡先担当者の役職の表記"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range xsd:string .

ic:担当者名 a owl:ObjectProperty ;
	rdfs:label "担当者名"@ja ;
	rdfs:comment "連絡先担当者の氏名"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range ic:氏名型 .

ic:Eメールアドレス a owl:DatatypeProperty ;
	rdfs:label "Eメールアドレス"@ja ;
	rdfs:comment "電子メールのメールアドレス"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range xsd:string .

ic:送付先 a owl:DatatypeProperty ;
	rdfs:label "送付先"@ja ;
	rdfs:comment "送付先の住所、私書箱など"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range xsd:string .

ic:電話番号 a owl:DatatypeProperty ;
	rdfs:label "電話番号"@ja ;
	rdfs:comment "連絡先の電話番号"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range ic:電話番号型 .

ic:内線番号 a owl:DatatypeProperty ;
	rdfs:label "内線番号"@ja ;
	rdfs:comment "連絡先の内線番号"@ja ;
	schema:domainIncludes ic:連絡先型 ;
	rdfs:range xsd:string .

ic:FAX番号 a owl:DatatypeProperty ;
	rdfs:label "FAX番号"@ja ;
	rdfs:comment "連絡先のFAX番号など"@ja ;
