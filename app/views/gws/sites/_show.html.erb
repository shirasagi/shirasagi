<dl class="see">
  <dt><%= @model.t :name %></dt>
  <dd><%= @item.name %></dd>

  <% if @item.domains.present? %>
  <dt><%= @model.t :domains %></dt>
  <dd><%= @item.domains %></dd>
  <% end %>

  <dt><%= @model.t :path_id %></dt>
  <dd><%= @item.path_id %></dd>

  <dt><%= t("ss.login_url") %></dt>
  <dd>
    <% @item.domains.each do |domain| %>
      <%= link_to gws_login_url(domain: domain), gws_login_url(domain: domain) %><br />
    <% end %>
    <%= link_to gws_login_url, gws_login_url %><br />
  </dd>
</dl>
