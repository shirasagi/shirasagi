<div class="list-head">
  <label class="check"><input type="checkbox" /></label>

  <div class="list-head-action">
    <%= render template: "_list_head_tags" %>
    <div class="list-head-action-destroy">
      <%= ss_button_to t("ss.links.delete"), url_for(action: :soft_delete_all), model: @model, class: "disable-all btn btn-list-head-action", method: "post", confirm: t('ss.confirm.soft_delete') %>
    </div>
  </div>

  <%= render template: "_search" %>
</div>
