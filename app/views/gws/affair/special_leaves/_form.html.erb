<% staff_category_options = SS.config.gws.user["staff_category"] || [] %>
<dl class="see">
  <dt><%= @model.t :code %><%= @model.tt :code %></dt>
  <dd><%= f.text_field :code %></dd>

  <dt><%= @model.t :name %><%= @model.tt :name %></dt>
  <dd><%= f.text_field :name %></dd>

  <dt><%= @model.t :order %><%= @model.tt :order %></dt>
  <dd><%= f.number_field :order %></dd>

  <dt><%= @model.t :staff_category %><%= @model.tt :staff_category %></dt>
  <dd><%= f.select :staff_category, staff_category_options, include_blank: "" %></dd>
</dl>
