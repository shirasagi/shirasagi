<dl class="see">
  <dt><%= t('ss.encoding') %></dt>
  <dd>
    <% %w(UTF-8 Shift_JIS).each do |encoding| %>
      <label>
        <%= radio_button_tag "#{f.object_name}[encoding]", encoding, encoding == 'UTF-8', {id: nil} %>
        <%= t("ss.options.csv_encoding.#{encoding}") %>
      </label>
    <% end %>
  </dd>
  <dd>
    <label><%= check_box_tag "webmail_support", "true", params[:webmail_support].present?, id: nil %> <%= t("gws.include_webmail_role") %></label>
  </dd>
</dl>
