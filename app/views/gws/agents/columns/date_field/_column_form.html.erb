<% value ||= nil %>
<%= render file: 'gws/agents/columns/main/_common_label', locals: { column: column } %>
<dd>
  <%= column.prefix_label %>
  <%= text_field_tag("#{object_name}[#{column.id}]", tryb { value.try(:date).strftime('%Y/%m/%d') }, column.form_options) %>
  <%= column.postfix_label %>
</dd>
