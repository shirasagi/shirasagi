<% value ||= nil %>
<%= render file: 'gws/agents/columns/main/_common_label', locals: { column: column } %>
<dd>
  <div class="input-group date-field-column">
    <span class="prefix input-group-prepend"><%= column.prefix_label %></span>
    <% format = (column.input_type == "datetime") ? '%Y/%m/%d %H:%M' : '%Y/%m/%d' %>
    <%= text_field_tag("#{object_name}[#{column.id}]", tryb { value.date.strftime(format) }, column.form_options) %>
    <span class="postfix input-group-append"><%= column.postfix_label %></span>
  </div>
</dd>
