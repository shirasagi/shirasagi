<% value ||= nil %>
<%= render file: 'gws/agents/columns/main/_common_label', locals: { column: column } %>
<dd>
  <span class="prefix"><%= column.prefix_label %></span>
  <% column.select_options.each do |val| %>
    <label>
      <%= radio_button_tag("#{object_name}[#{column.id}]", val, value.try(:value) == val, column.form_options) %>
      <%= val %>
    </label>
  <% end %>
  <span class="postfix"><%= column.postfix_label %></span>
</dd>
