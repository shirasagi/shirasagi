<% if @model.member_include_custom_groups? && @item.member_custom_groups.present? %>
<dl class="see">
  <dt><%= @model.t :member_custom_group_ids %></dt>
  <dd><%=br @item.member_custom_groups.map(&:name) %></dd>
</dl>
<% end %>

<dl class="see">
  <dt><%= @model.t :member_group_ids %></dt>
  <dd><%= br @item.member_groups.active.map(&:name) %></dd>
</dl>

<dl class="see">
  <dt><%= @model.t :member_ids %></dt>
  <dd><%=br gws_public_user_long_names(@item.sorted_members.map(&:long_name)) %></dd>
</dl>
