<% return if @item.main_facility.blank? %>

<dl class="see mod-gws-schedule-facility_custom_values">
  <% @item.main_facility.custom_fields.order_by(order: 1, created: 1).each do |custom_field| %>
  <dt><%= custom_field.name %></dt>
  <dd><%= tryb { br [ @item.facility_custom_values[custom_field.id.to_s]['value'].presence ].compact.join(', ') } %></dd>
  <% end %>
</dl>
