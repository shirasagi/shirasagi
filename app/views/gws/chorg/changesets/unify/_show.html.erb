<dl class="see after_move">
  <dt><%= t "chorg.views.#{params[:type]}_changesets.after_unify" %></dt>
  <dd></dd>
  <% @model::GROUP_ATTRIBUTES.each do |k| %>
    <dt><%= t "chorg.views.#{params[:type]}_changesets.#{k}" %></dt>
    <dd><%= @item.destinations.try(:first).try(:send, :[], k) %></dd>
  <% end %>
</dl>

<dl class="see before_move">
  <dt><%= t "chorg.views.#{params[:type]}_changesets.before_unify" %></dt>
  <dd></dd>
  <dt><%= t "chorg.views.#{params[:type]}_changesets.name" %></dt>
  <dd>
    <%= render SS::MoreContentComponent.new(cur_site: @cur_site, cur_user: @cur_user, max_height: "20em") do %>
      <%= br @item.sources.map { |e| e["name"] } %>
    <% end %>
  </dd>
</dl>
