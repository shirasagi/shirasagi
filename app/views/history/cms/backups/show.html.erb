<div class="main-box">
  <table class="see history-backup table table-striped">
    <% @item.data.sort.each do |k, v| %>
    <tr>
      <th><%= k %></th>
      <td class="<%= 'diff' if @data[k] != v %>">
        <%= v.present? ? v.inspect : "" %>
      </td>
    </tr>
    <% end %>
  </table>
</div>
