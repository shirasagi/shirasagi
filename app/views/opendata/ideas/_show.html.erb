<dl class="see">
  <dt><%= @model.t :name %></dt>
  <dd><%= @item.name %></dd>

  <dt><%= @model.t :text %></dt>
  <dd><%= @item.text %></dd>

  <dt><%= @model.t :tags %></dt>
  <dd><%= @item.tags %></dd>

  <dt><%= @model.t :dataset_id %></dt>
  <dd>
    <% if @item.dataset %>
      <%= @item.dataset.name %>
    <% end %>
  </dd>

  <dt><%= @model.t :app_id %></dt>
  <dd>
    <% if @item.app %>
      <%= @item.app.name %>
    <% end %>
  </dd>

  <dt><%= @model.t :point %></dt>
  <dd><%= @item.point %></dd>
</dl>
