<dl class="see">
  <dt><%= @model.t :name %><%= @item.tt :name %><%= required_label %></dt>
  <dd><%= f.text_field :name %></dd>

  <dt><%= @model.t :url %><%= @item.tt :url %><%= required_label %></dt>
  <dd><%= f.text_field :url %></dd>

  <dt><%= @model.t :api_type %><%= @model.tt :api_type %><%= required_label %></dt>
  <dd><%= f.select :api_type, @item.api_type_options %></dd>

  <dt><%= @model.t :api_key %><%= @item.tt :api_key %><%= required_label %></dt>
  <dd><%= f.text_field :api_key %></dd>

  <dt><%= @model.t :order %><%= @model.tt :order %></dt>
  <dd><%= f.number_field :order %></dd>
</dl>
