<div>
  <% @estat_categories.each do |item| %>
  <article>
    <header>
      <h2><a href="<%= @search_path.call("s[estat_category_id]" => "#{item.id}") %>">
      <span class="name"><%= item.name %></span>
      <span class="count">(<%= item.count %>)</span></a></h2>
    </header>
  </article>
  <% end %>
  <% if @estat_categories.count == 0 %>
    <div><%= I18n.t("opendata.labels.not_data") %></div>
  <% end %>
</div>
