<%= form_for :item, url: { action: :import }, html: { id: "item-form", multipart: true, autocomplete: :off } do |f| %>
  <%= error_messages_for :item %>

<section class="main-box">
  <header><h2><%= t("rss.confirm.import") %></h2></header>
</section>

<footer class="send">
  <%= f.submit t("rss.buttons.import"), class: :save %>
  <%= f.button t("ss.buttons.cancel"), type: :reset, onclick: "location.href='#{url_for(action: :index)}'; return false;" %>
</footer>
<% end %>
