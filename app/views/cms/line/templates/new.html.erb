<% @crud_buttons = proc do |f| %>
  <%= f.submit t("ss.buttons.save"), class: 'btn-primary save', data: { disable_with: t("ss.buttons.saving") } %>
  <%= f.button t("ss.buttons.cancel"), class: 'btn-default', type: :reset, onclick: "location.href='#{url_for(action: :select_type, type: "-")}'; return false;" %>
<% end %>

<%= render template: "ss/crud/new" %>
