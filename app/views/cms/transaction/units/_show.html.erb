<dl class="see">
  <dt><%= @model.t :name %></dt>
  <dd><%= @item.name %></dd>

  <dt><%= @model.t :type %></dt>
  <dd><%= @item.label :type %></dd>

  <dt><%= @model.t :order %></dt>
  <dd><%= @item.order %></dd>

  <dt><%= @model.t :execute_at %></dt>
  <dd><%= l(@item.execute_at, format: :picker) rescue nil %></dd>
</dl>
