<dl class="see mod-cms-column-select-like">
  <% if @model.use_parent_column_name %>
    <dt><%= @model.t :parent_column_name %><%= @model.tt :parent_column_name %></dt>
    <dd><%= f.text_field :parent_column_name %></dd>
  <% end %>

  <dt><%= @model.t :select_options %><%= @model.tt :select_options %></dt>
  <dd><%= f.ss_lines_field :select_options, value: @item.select_options.join("\n") %></dd>
</dl>
