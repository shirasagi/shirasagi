<% value ||= nil %>
<% controller ||= false %>
<%= render 'cms/agents/columns/main/base', f: f, column: column, value: value, controller: controller do %>
  <%= text_field_tag("#{f.object_name}[][in_wrap][label]", value.try(:label), column.form_options(:label).merge(id: nil, style: "width: 30%;")) %>
  <%= url_field_tag("#{f.object_name}[][in_wrap][link]", value.try(:link), column.form_options(:link).merge(id: nil, style: "width: 50%;")) %>
<% end %>
