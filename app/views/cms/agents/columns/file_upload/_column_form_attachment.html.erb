<% name = "#{f.object_name}[][in_wrap][file_label]" %>

<%= jquery do %>
Cms_Form.addSyntaxCheck("<%= id %>", {
  resolve: "text",
  name: "<%= column.name %>",
  getContent: function () { return $('#<%= id %> [name="<%= name %>"]').val(); },
  setContent: function (content) { return $('#<%= id %> [name="<%= name %>"]').val(content); },
});
<% end %>

<%= text_field_tag(name, value.try(:file_label), id: nil, "data-check-presence": t("cms.column_file_upload.attachment.file_label"), placeholder: t("cms.column_file_upload.attachment.file_label_place_holder")) %>
