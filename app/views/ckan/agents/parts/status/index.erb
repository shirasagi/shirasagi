<div class="<%= @cur_part.ckan_status %>">
  <% if @cur_part.ckan_value_url.present? %>
  <%= link_to @cur_part.ckan_value_url do %>
  <span><%= @cur_part.value %></span>
  <span><%= @cur_part.label :ckan_status %></span>
  <% end %>
  <% else %>
  <span><%= @cur_part.value %></span>
  <span><%= @cur_part.label :ckan_status %></span>
  <% end %>
</div>
