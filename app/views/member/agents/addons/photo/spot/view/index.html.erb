<% map_points = @cur_page.member_photos.and_public.map(&:map_points).select(&:present?).flatten %>
<% return if map_points.blank? %>

<%= javascript_include_tag "//maps.googleapis.com/maps/api/js?v=3&sensor=false" %>
<%= jquery do %>
  Map.load("#map-canvas");
  Map.setMarkers(<%= raw map_points.to_json %>);
<% end %>
<div id="map-canvas" style="width: 100%; height: 400px;"></div>
