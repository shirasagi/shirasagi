  Syntax_Checker.correctAdjacentA = function (a) {
    var next;
    next = $(a).next('a[href="' + $(a).attr("href") + '"]');
    if (next.length) {
      if ($(a).html() !== $(next).html()) {
        $(a).html($(a).html() + $(next).html());
