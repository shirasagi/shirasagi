        } else {
          return date;
        }
      });
      return this.textContent = text;
    });
    return ret.html();
  };

  Syntax_Checker.checkAdjacentA = function (html) {
    return $(html).find("a[href]").each(function () {
      var errors, next;
      next = $(this).next('a[href="' + $(this).attr("href") + '"]');
